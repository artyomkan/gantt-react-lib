{
  "bracketSameLine": false,
  "bracketSpacing": true,
  "cssDeclarationSorterOrder": "alphabetical",
  "jsxSingleQuote": true,
  "plugins": [
    "prettier-plugin-sort-json",
    "prettier-plugin-organize-imports",
    "prettier-plugin-css-order"
  ],
  "printWidth": 80,
  "proseWrap": "preserve",
  "quoteProps": "consistent",
  "semi": true,
  "singleQuote": true,
  "tabWidth": 2,
  "trailingComma": "es5"
}
